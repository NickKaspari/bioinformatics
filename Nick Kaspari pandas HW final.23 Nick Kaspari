$ grep -v "Zoo" pandas.txt >> pandas-zoo
$ awk '{ print $1,$3,$2}' pandas.txt >> pandas_sort_columns
$ cat pandas.txt | awk '{print $5}' | sort >> ZOONAMESSORT
$ sed -i 's/male/M/g' | sed -i 's/female/F/g' pandas.txt >> MALE_M_FEMALE_F
$ grep -c 'October\|Female' pandas.txt
$ cat -n pandas.txt >>LINE_NUMBERS
$ sed -n '1p;6p;17p' pandas.txt >> 1_6_17_lines
$ grep -i "x" p pandas.txt >>PANDAS_WITH_X
$ awk '{print$5}' pandas.txt | sort -u
$ awk '$4 < 10 {print$1,$4;}' pandas.txt | sort -k2 >> question_10